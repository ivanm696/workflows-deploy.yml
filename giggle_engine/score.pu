import datetime

def map_to_note(text):
    """–ü—Ä–µ–æ–±—Ä–∞–∑—É–µ—Ç —Ç–µ–∫—Å—Ç –≤ –º—É–∑—ã–∫–∞–ª—å–Ω—ã–µ –Ω–æ—Ç—ã"""
    notes = ['C', 'D', 'E', 'F', 'G', 'A', 'B']
    return [notes[ord(c) % len(notes)] for c in text if c.isalpha()]

def generate_score(logfile="breath.log"):
    """–°–æ–∑–¥–∞—ë—Ç –ø–∞—Ä—Ç–∏—Ç—É—Ä—É –∏–∑ –¥—ã—Ö–∞–Ω–∏–π"""
    score = []
    try:
        with open(logfile, "r", encoding="utf-8") as f:
            lines = f.readlines()
            for line in lines:
                timestamp = line[:16]
                ritual = line.strip()[20:]
                notes = map_to_note(ritual)
                score.append(f"{timestamp} üéµ {'-'.join(notes)}")
    except FileNotFoundError:
        print("‚ùå breath.log –Ω–µ –Ω–∞–π–¥–µ–Ω")
    return score

if __name__ == "__main__":
    print("üéº –ü–∞—Ä—Ç–∏—Ç—É—Ä–∞ –¥—ã—Ö–∞–Ω–∏—è Nicu:")
    for line in generate_score():
        print(line)
